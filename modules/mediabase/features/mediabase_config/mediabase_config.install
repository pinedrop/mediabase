<?php

/**
* Enabling / Disableing modules and themes
**/
function mediabase_config_install() {
   //Enable themes
   db_query("UPDATE {system} SET status = 1 WHERE type = 'theme' AND name in (  'mb_html5', 'omega') ");
   //Disable modules
   db_query("UPDATE {system} SET status = 0 WHERE type = 'module' AND name in (  'overlay' ) ");
}
